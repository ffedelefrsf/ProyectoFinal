	<queryString>
		<![CDATA[select table_c.id, table_c.dni, table_c.apellido, table_c.nombre, table_c.telefono, table_c.FECHA_COBERTURA, IF(table_c.FECHA_COBERTURA <= current_date(), "SI", "NO") as CUBIERTO, e.DESCRIPCION
from (
select *
from socios s
union 
select *
from adherentes a2
) as table_c
inner join estados e on e.ID = table_c.ID_ESTADO 
order by table_c.dni]]>
	</queryString>
