	<queryString>
		<![CDATA[-- chart 2
select SUM(C.IMPORTE_TOTAL) as importe_total, SUM(P.VALOR) as pago
from comprobantes c
inner join comprobantes_audit ca on c.ID = ca.ID and month(ca.FECHA) = month(current_date()) and year(ca.FECHA) = year(current_date()) 
left join pagos p on p.ID_COMPROBANTE = c.ID 
left join comprobantes c1 on p.ID_COMPROBANTE = c1.ID ]]>
	</queryString>
