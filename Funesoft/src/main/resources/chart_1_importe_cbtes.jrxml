	<queryString>
		<![CDATA[-- chart 1
select IF(ISNULL(P.ID), "DEBE", "PAGO") as descripcion, COUNT(IF(ISNULL(P.ID), "DEBE", "PAGO")) as cuenta
from comprobantes c
inner join comprobantes_audit ca on c.ID = ca.ID and month(ca.FECHA) = month(current_date()) and year(ca.FECHA) = year(current_date()) 
left join pagos p on p.ID_COMPROBANTE = c.ID 
group by IF(ISNULL(P.ID), "DEBE", "PAGO")]]>
	</queryString>
